{
  "name": "preact-prop-helpers",
  "type": "module",
  "version": "0.0.1",
  "description": "A small set of hooks and other functions related to modifying Preact props.",
  "source": "src/index.tsx",
  "module": "/dist/index.js",
  "types": "/dist/index.d.ts",
  "packageManager": "pnpm@7.26.2",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "umd": "./dist/index.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npx npm-run-all --npm-path npm -pc clean build:*",
    "watch": "npx npm-run-all --npm-path npm -pc clean watch:*",
    "watch:ts": "tsc --watch",
    "build:ts": "tsc",
    "test": "jest",
    "lint": "npx eslint ./src",
    "prepare": "npm run build || node ./true",
    "clean": "tsc --build --clean"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "preact": "^10.13.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "blocking-elements": "^0.1.1",
    "clsx": "^1.2.1",
    "lodash-es": "^4.17.21",
    "tabbable": "^6.0.1",
    "wicg-inert": "^3.1.2"
  },
  "directories": {
    "test": "tests"
  }
}
