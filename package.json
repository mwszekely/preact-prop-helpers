{
  "name": "preact-prop-helpers",
  "type": "module",
  "version": "0.0.1",
  "description": "A small set of hooks and other functions related to modifying Preact props.",
  "source": "src/index.tsx",
  "module": "/dist/index.js",
  "types": "/dist/index.d.ts",
  "packageManager": "pnpm@8.6.12",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/index.react.d.ts",
      "browser": "./dist/index.react.js",
      "import": "./dist/index.react.js"
    },
    "./package.json": "./package.json"
  },
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "npx npm-run-all --npm-path npm -pc clean build:*",
    "watch": "npx npm-run-all --npm-path npm -pc clean watch:*",
    "watch:preact": "tsc --watch",
    "build:preact": "tsc",
    "watch:react": "npx rollup -c ./rollup.config.js --watch --no-watch.clearScreen",
    "build:react": "npx rollup -c ./rollup.config.js",
    "test": "playwright test --ui",
    "lint": "npx eslint ./src",
    "clean": "tsc --build --clean",
    "rebuild": "rm \"./pnpm-lock.yaml\" && rm -R \"./node_modules\" && pnpm install && pnpm run build"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "preact": "^10.13.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/lodash-es": "^4.17.8",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.28.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-ts": "^3.4.4",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "async-to-sync": "github:mwszekely/async-to-sync",
    "blocking-elements": "^0.1.1",
    "clsx": "^2.0.0",
    "lodash-es": "^4.17.21",
    "map-and-set-extensions": "github:mwszekely/map-and-set-extensions",
    "tabbable": "^6.2.0",
    "wicg-inert": "^3.1.2"
  },
  "directories": {
    "test": "tests"
  }
}
