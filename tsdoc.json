{
    "$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",
    "tagDefinitions": [
      {
        "tagName": "@docTopic",
        "syntaxKind": "block",
        "allowMultiple": true
      },
      // When followed immediately by a @link tag,
      // generates the documentation for that item
      // and includes it in the current markup generation.
      {
        "tagName": "@include",
        "syntaxKind": "inline",
        "allowMultiple": true
      },
      // Refers to the child version of a function (e.g. useRovingTabIndexChild)
      {
        "tagName": "@hasChild",
        "syntaxKind": "block",
        "allowMultiple": true
      },
      {
        // Indicates that this is one of those special functions with special parameter handling
        "tagName": "@compositeParams",
        "syntaxKind": "modifier",
        "allowMultiple": false
      },
      // In @packageDocumentation, helps to build the ToC.
      // Format: {@tableOfContents [start|end|header|subheader] [header/subheader content]}
      // e.g. {@tableOfContents subheader This is the subheader}
      {
        "tagName": "@tableOfContents",
        "syntaxKind": "inline",
        "allowMultiple": true
      }
    ]
  }